{
    "name": "Heroku Pluto Slack bot",
    "description": "Experimental Veramo Slack bot",
    "repository": "https://github.com/veramolabs/pluto-slack-bot",
    "keywords": [
      "node",
      "veramo",
      "identity",
      "verifiable-credentials",
      "slack"
    ],
    "env": {
      "AGENT_URL": {
        "description": "Agent URL",
        "value": "https://sun.veramo.io/agent"
      },
      "AGENT_API_KEY": {
        "description": "Agent API key",
        "value": ""
      },
      "SLACK_BOT_DID_ALIAS": {
        "description": "Slack bot DID alias",
        "value": "sun.veramo.io:pluto"
      },
      "SLACK_TOKEN": {
        "description": "Slack token",
        "value": ""
      }
    }
  }